@model Book


<div class="container mt-5">
    <h1>DELETE BOOK DETAILS</h1>
    <input  asp-for="Id" hidden />
    <form method="post">
        <div class="form-group">
            <label asp-for="Author"></label>
            <input asp-for="Author" disabled class="form-control" >
        </div>
        <div class="form-group">
            <label asp-for="Title"></label>
            <input asp-for="Title" disabled class="form-control" >
        </div>
        <div class="form-group">
            <label asp-for="Description"></label>
            <textarea asp-for="Description" disabled class="form-control" ></textarea>
        </div>
        <button type="submit" class="btn btn-dark">DELATE</button>
    </form>
</div>


